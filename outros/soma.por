programa {
  funcao inicio() {
    inteiro numeroA
    inteiro numeroB
    inteiro soma
    escreva("Digite o primeiro número a ser somado", "\n")
    leia(numeroA)
    escreva("Digite o segundo número a ser somado", "\n")
    leia(numeroB)

    soma = numeroA + numeroB


    escreva("O total é ", soma)
  }
}
